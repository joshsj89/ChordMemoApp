            "package": "com.joshsj89.ChordMemo",
            "versionCode": 38,
            "permissions": [
                "WRITE_EXTERNAL_STORAGE",
                "READ_EXTERNAL_STORAGE"
            ]
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "eas": {
                "projectId": "2d65ac9e-072f-47a6-990d-a7cd3d113127"
            }
        },
        "runtimeVersion": {
            "policy": "appVersion"
        },
        "updates": {
